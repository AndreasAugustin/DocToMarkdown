<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
  <appSettings>

	<!-- The absolute path to the folder where the doc xml files are located -->
    <add key="xmlSource.folder.path" value=""/>

    <!-- The absolute path to the folder where the markup files should be safed -->
    <add key="markupTarget.folder.path" value=""/>

    <!-- Define here which kind of markdown should be created -->
	    <!-- 0 for GitHub flavored markdown (gfm) -->
	    <!-- 1 for standard markdown -->
	    <!-- 2 for classic markdown -->
	    <!-- 3 for extra markdown -->
   	<add key="markdownType" value="0"/>

   	<!-- Define the global threshold for the log level.-->
   	<!-- Loglevels below this threshold are not logged. -->
		<!-- 0 for off -->
	    <!-- 1 for fatal -->
	    <!-- 2 for error -->
	    <!-- 3 for warn -->
	    <!-- 4 for info -->
	    <!-- 5 for debug -->
	    <!-- 6 for trace -->
   	<add key="logger.global.threshold" value="4"></add>
  </appSettings>
</configuration>